            </StackPanel>
        </Button>

        <Button  HorizontalAlignment="Left" Margin="287,544,0,0" VerticalAlignment="Top" Width="131" Height="53" FontSize="20">
            <StackPanel Orientation="Horizontal">
                <Image Source="Resources/icons8-download-from-the-cloud-64.png" Height="35"/>
                <Label Content="Guardar"/>
            </StackPanel>
        </Button>
        <Button  HorizontalAlignment="Left"  Margin="554,544,0,0" VerticalAlignment="Top" Width="131" Height="53" FontSize="20">
            <StackPanel Orientation="Horizontal">
                <Image Source="Resources/icons8-remove-100.png" Height="35"/>
                <Label Content="Eliminar"/>
            </StackPanel>
        </Button>
        <TextBox x:Name="___Sin_nombre_" HorizontalAlignment="Left" Height="23" Margin="216,51,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" FontSize="18"/>
