<Window x:Class="Formulario1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Formulario1"
        mc:Ignorable="d"
        Title="Formulario1" Height="700" Width="800">
    <Grid Margin="0,0,-8,-216" Height="890" VerticalAlignment="Top">
        <Grid.RowDefinitions>
            <RowDefinition Height="198*"/>
            <RowDefinition/>
            <RowDefinition Height="69*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Label Content="EntradaID :" HorizontalAlignment="Left" Margin="54,51,0,0" VerticalAlignment="Top" Height="37" Width="109" FontSize="20"/>
        <Label Content="fecha :" HorizontalAlignment="Left" Margin="54,109,0,0" VerticalAlignment="Top" Height="36" Width="83" FontSize="20"/>
        <Label Content="SuplidorID :" HorizontalAlignment="Left" Margin="54,169,0,0" VerticalAlignment="Top" Width="109" FontSize="20" Height="56"/>
        <Label Content="cacaoID :" HorizontalAlignment="Left" Margin="54,225,0,0" VerticalAlignment="Top" Height="41" Width="83" FontSize="20"/>
        <Label Content="cantidad :" HorizontalAlignment="Left" Margin="54,291,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="41" Width="100" FontSize="20">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.642"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label Content="costo :" HorizontalAlignment="Left" Margin="54,350,0,0" VerticalAlignment="Top" Height="30" Width="83" FontSize="20"/>
        <Label Content="Total :" HorizontalAlignment="Left" Margin="54,414,0,0" VerticalAlignment="Top" Height="39" Width="83" FontSize="20"/>
