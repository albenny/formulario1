        <DatePicker HorizontalAlignment="Left" Margin="216,109,0,0" VerticalAlignment="Top" Height="36" Width="211" FontSize="18"/>
        <ComboBox HorizontalAlignment="Left" Margin="216,169,0,0" VerticalAlignment="Top" Width="211" Height="31" FontSize="18"/>
        <ComboBox HorizontalAlignment="Left" Margin="212,225,0,0" VerticalAlignment="Top" Width="215" Height="41" FontSize="18"/>
        <Button  HorizontalAlignment="Left" Margin="595,35,0,0" VerticalAlignment="Top" Width="131" Height="53" FontSize="20">
            <StackPanel Orientation="Horizontal">
                <Image Source="Resources/icons8-search-64.png" Height="35"/>
                <Label Content="buscar"/>
            </StackPanel>
        </Button>
        <Label Content="consulltar Creacion" HorizontalAlignment="Left" Margin="565,113,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Height="42" Width="193" FontSize="20">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.189"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        
        <Button  HorizontalAlignment="Left" Margin="595,169,0,0" VerticalAlignment="Top" Width="131" Height="56" FontSize="20">
            <StackPanel Orientation="Horizontal">
                <Image Source="Resources/icons8-search-64.png" Height="35"/>
                <Label Content="consultar"/>
            </StackPanel>
        </Button>
        <TextBox HorizontalAlignment="Left" Height="39" Margin="163,414,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="173" FontSize="18"/>
        <Label Content="Fecha creacion :" HorizontalAlignment="Left" Margin="489,295,0,0" VerticalAlignment="Top" FontSize="18"/>
        <Label Content="09/09/20" HorizontalAlignment="Left" Margin="489,334,0,0" VerticalAlignment="Top" Height="31" Width="114" FontSize="18"/>
        <Label Content="Fecha modificacion :" HorizontalAlignment="Left" Margin="489,370,0,0" VerticalAlignment="Top" Width="176" FontSize="18" Height="41"/>
        <Label Content="09/0920" HorizontalAlignment="Left" Margin="489,419,0,0" VerticalAlignment="Top" Width="114" FontSize="18"/>
        <TextBox HorizontalAlignment="Left" Height="38" Margin="186,350,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="165" FontSize="18"/>


    </Grid>
</Window>
